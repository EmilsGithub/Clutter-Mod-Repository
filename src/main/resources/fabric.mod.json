{
	"schemaVersion": 1,
	"id": "clutter",
	"version": "${version}",
	"name": "Clutter",
	"description": "Adds a bunch of things like decorations, mobs, items and more.",
	"authors": [
		"EmilSG"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/clutter",
		"issues": "https://github.com/EmilsGithub/Clutter-Mod-Repository/issues",
		"sources": "https://github.com/EmilsGithub/Clutter-Mod-Repository"
	},
	"custom": {
		"modmenu": {
			"links": {
				"Discord": "https://discord.com/invite/pDMkMxMREA",
